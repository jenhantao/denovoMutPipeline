#! /usr/bin/env python

import sys

f = file(sys.argv[1],'r')
fdata = f.readlines()
f.close()

for line in fdata:
    if line.startswith("#"):
       print line.strip()
    else:
	row = line.strip().split("\t")
	ref = row[2]
	alt = row[3].split(",")
	for alt_allele in alt:
	    if alt_allele == ref:
	       continue
	    else:
		print "\t".join(["\t".join(row[:2]),alt,"\t".join(row[3:])])